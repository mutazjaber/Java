package Mammal;
public class Gorilla extends Mammal {

	public Gorilla(int energy) {
		super(energy);
	
	}
public void Bannana() {
	energy-=5;
	System.out.println(" that the gorilla has thrown  banan "+energy);
}
public void throwBannana() {
	energy+=10;
	System.out.println("the Gorilla is satisfied "+energy);
	
}
public void climb() {
	energy-=10;
	System.out.println("the Gorilla is not satisfied "+energy);
	
	}
}
